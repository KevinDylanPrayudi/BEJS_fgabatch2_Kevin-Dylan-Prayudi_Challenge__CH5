{
  "name": "basic-banking-system",
  "version": "1.0.0",
  "description": "This project to accomplish BINAR's Challenge Chapter 4",
  "main": "index.js",
  "scripts": {
    "start:prod": "node index.js",
    "start:dev": "nodemon index.js",
    "test": "jest --forceExit"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "text"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/prisma/",
      "/coverage/",
      "/db/",
      "/public/",
      "/middlewares/",
      "/tests/api.*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/prisma/",
      "/coverage/",
      "/db/",
      "/public/",
      "/middlewares/",
      "\/routes(\/(\\w)*)*\/validator"
    ]
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^1.3.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prisma": "^5.16.2",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "repository": {
    "bugs": {
      "url": "https://github.com/KevinDylanPrayudi/BEJS_fgabatch2_KevinDylanPrayudi_ChallengeChapter4/issues"
    },
    "homepage": "https://github.com/KevinDylanPrayudi/BEJS_fgabatch2_KevinDylanPrayudi_ChallengeChapter4#readme"
  }
}